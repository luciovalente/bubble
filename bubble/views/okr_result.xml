<odoo>
    <record id="view_okr_result_tree" model="ir.ui.view">
        <field name="name">okr.result.tree</field>
        <field name="model">okr.result</field>
        <field name="arch" type="xml">
            <tree editable="bottom">
                <field name="okr_id"/>
                <field name="date"/>
                <field name="evaluation_id"/>
                <field name="result"/>
                <field name="result_char"/>
            </tree>
        </field>
    </record>


    <menuitem id="menu_okr_result" name="OKR Results" parent_id="menu_objective_main" sequence="6"/>
        <record id="action_okr_result" model="ir.actions.act_window">
            <field name="name">OKR Results</field>
            <field name="res_model">okr.result</field>
            <field name="view_mode">tree</field>
        </record>
        <menuitem id="menu_okr_result_list" name="OKR Results" parent_id="menu_okr_result"
                  action="action_okr_result" sequence="7"/>

    
</odoo>
