<odoo>
    <record id="view_buble_type_tree" model="ir.ui.view">
        <field name="name">bubble.type.tree</field>
        <field name="model">bubble.type</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="company_id"/>
            </tree>
        </field>
    </record>

    <record id="view_buble_type_form" model="ir.ui.view">
        <field name="name">bubble.type.form</field>
        <field name="model">bubble.type</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name"/>
                    <field name="description"/>
                    <field name="company_id"/>
                    <field name="role_ids"/>
                    <field name="model_id"/>
                    <field name="code"/>
                </group>
            </form>
        </field>
    </record>

    <menuitem id="menu_buble_type" name="Buble Types" parent_id="menu_bubble_main" sequence="4"/>
        <record id="action_buble_type" model="ir.actions.act_window">
            <field name="name">Buble Types</field>
            <field name="res_model">bubble.type</field>
            <field name="view_mode">tree,form</field>
        </record>
        <menuitem id="menu_buble_type_list" name="Buble Types" parent_id="menu_buble_type"
                  action="action_buble_type" sequence="5"/>
</odoo>
